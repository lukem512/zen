nav.navbar.navbar-default.navbar-fixed-top.navbar-shrink
	.container
		.navbar-header.page-scroll
			button.navbar-toggle(type="button", data-toggle="collapse", data-target="#page-list")
				span.sr-only Toggle navigation
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href="#page-top")= name

		#page-list.collapse.navbar-collapse
			ul.nav.navbar-nav.navbar-right
				each page, i in pages
					li.page-scroll
						a(href="#{page.href}")= page.name

if header
	header
		.container
			!= header.content
